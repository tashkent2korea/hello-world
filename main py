def korea_preparation():
    """Koreyaga tayyorgarlik dasturi"""
    print("\n" + "="*50)
    print("ğŸ‡°ğŸ‡· KOREA 2+2 PREPARATION PROGRAM")
    print("="*50)
    
    # Ma'lumotlar
    name = "tashkent2korea"
    country = "Uzbekistan"
    goal = "Study Computer Science in Korea"
    
    print(f"\nğŸ‘¤ Name: {name}")
    print(f"ğŸŒ Country: {country}")
    print(f"ğŸ¯ Goal: {goal}")
    
    # Vazifalar
    print("\nğŸ“‹ Tasks to complete:")
    tasks = [
        "1. Learn Python programming",
        "2. Master GitHub",
        "3. Study Korean language",
        "4. Build portfolio",
        "5. Apply for universities"
    ]
    
    for task in tasks:
        print(f"   {task}")
    
    # Progress
    print("\nğŸ“Š Current progress:")
    print("   âœ… GitHub account created")
    print("   âœ… First repository made")
    print("   âœ… Profile README created")
    print("   ğŸ”² Learning Python daily")
    print("   ğŸ”² Korean language study")
    
    print("\n" + "="*50)
    print("ğŸš€ Keep going! Every day progress!")
    print("="*50)

